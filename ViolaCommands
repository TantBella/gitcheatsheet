46. git reset --hard
47. git reset [file]
48. git revert [commit]
49. git rm <file>
50. git shortlog
51. git show <commit>
52. git stash
53. git stash apply
54. git stash list
55. git stash pop
56. git status
57. git submodule add <repo_url> <path>
58. git submodule update --init
59. git tag <tagname>
60. git tag -a v1.0 -m "version 1.0 release"